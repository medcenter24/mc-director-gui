    "karma-jasmine-html-reporter": "^1.0.0",
    "npm-run-all": "^4.1.2",
    "protractor": "^5.3.0",
    "standard-changelog": "^1.0.14",
    "stylelint": "^9.1.3",
    "ts-helpers": "^1.1.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.7.2",
    "typogr": "^0.6.7",
    "underscore": "^1.8.3",
    "webpack-cli": "^2.0.13",
    "wintersmith": "^2.4.1",
    "wintersmith-sassy": "^1.2.0"
  }
