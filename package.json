    "leaflet": "^1.3.1",
    "leaflet-map": "^0.2.1",
    "lodash": "^4.17.5",
    "ng2-completer": "^1.6.3",
    "ng2-handsontable": "^2.1.0-rc.3",
    "ng2-slim-loading-bar": "^4.0.0",
    "ng2-smart-table": "^1.2.2",
    "ng2-tree": "^2.0.0-rc.9",
