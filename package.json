    "@angular/router": "6.0.0-rc.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "amcharts3": "^3.21.12",
    "ammap3": "^3.21.12",
    "angular-calendar": "^0.23.7",
    "angular2-datatable": "^0.6.0",
    "angular2-logger": "^0.7.0",
