    "codelyzer": "^4.2.1",
    "electron": "^1.8.4",
    "gh-pages": "^1.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
